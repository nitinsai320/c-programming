#include<stdio.h>
main ()
{
	int n,s1,s2,i;
	printf("How many interger numbers n\n");
	scanf("%d",&n);
	i=n;
	if(n>0)
	{
		printf("enter the first number\n");
		scanf("%d",&s1);
		n--;
		if(n>0)
		{
			for(n>=1;n--;)
			{
				printf("enter the next number\n");
				scanf("%d",&s2);
				if(s1>s2)
				s1=s2;
			}
		}
	}
	printf("The lowest number %d",i,s1);
}
